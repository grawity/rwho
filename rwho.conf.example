# vim: ft=dosini

agent.notify_url = http://rwho.cluenet.org/server.php
;agent.auth_username = [will use sys.fqdn if empty]
;agent.auth_password = hunter2
;agent.exclude_root = true
;agent.exclude_users = smith|guest\d+
;agent.include_users = rms
;agent.min_uid = 1000

;db.pdo_driver = mysql:host=localhost;dbname=rwho
;db.username =
;db.password =

# if set, web interface will add a finger gateway link
;finger.host = localhost
;finger.ldap.uri = ldapi:///
;finger.ldap.dn = uid=%s,ou=people,dc=cluenet,dc=org
;finger.ldap.attr = planFile

# trust these IP masks with private information
;privacy.allow_addr =
# allow queries from untrusted addresses
;privacy.allow_anonymous = true
# hide remote-host from anonymous queries
;privacy.hide_rhost = true

# only accept updates with Basic authentication
;server.auth_required = true

;sys.hostname =
;sys.fqdn =

;web.auth_method = saml
;web.ssp_autoload = /usr/share/simplesamlphp/lib/_autoload.php
;web.ssp_authsource = default-sp

;web.stylesheet = /custom.css
